    "@reduxjs/toolkit": "^2.6.0",
    "@types/react": "^19.0.10",
    "@types/react-dropzone": "^4.2.2",
    "aws-amplify": "^6.13.3",
    "axios": "^1.8.3",
    "concurrently": "^8.2.2",
    "disqus-react": "^1.1.6",
    "ethers": "^6.13.5",
